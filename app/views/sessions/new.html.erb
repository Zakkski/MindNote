<div class="song-card" style="background-image: linear-gradient(208.57deg, rgba(0, 0, 0, 0.5 ), rgba(0, 0, 0, 0.8)), url('<%= @practice.song.album_url %>')">
  <div class="song-card-info">
    <h3><%= @practice.song.title %></h3>
    <p><%= @practice.song.artist.name %></p>
  </div>
</div>
<div class="star-form">
  <h1>Rate Your Rehearsal!</h1>
  <%= form_for([@practice, @session]) do |f| %>
    <div class="star-row">
      <%= radio_button_tag(:star, "5", required: "required") %>
      <%= label_tag(:star_5, "") %>
      <%= radio_button_tag(:star, "4", required: "required") %>
      <%= label_tag(:star_4, "") %>
      <%= radio_button_tag(:star, "3", required: "required") %>
      <%= label_tag(:star_3, "") %>
      <%= radio_button_tag(:star, "2", required: "required") %>
      <%= label_tag(:star_2, "") %>
      <%= radio_button_tag(:star, "1", required: "required") %>
      <%= label_tag(:star_1, "") %>
    </div>
    <%= f.submit "Finish Rehearsal", class: "mn-btn-lrg mn-btn-primary" %>
<% end %>
</div>
