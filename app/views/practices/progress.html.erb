<%= render 'shared/navbar' %>


     <div class="scroll-list">
        <div class="stat-card center">
          <p>image</p>
          <h2>Joined</h2>
          <p><%= current_user.created_at.strftime("%A, %b %d") %></p>
       </div>


       <div class="stat-card center" id="slide-1">
         <p>image</p>
         <h2> Songs practiced</h2>
         <p><%= (@practices.select { |practice| practice.sessions.count > 0 }).count  %></p>
       </div>

       <div class="stat-card center" id="slide-2">
         <p>image</p>
         <h2>Repertoire</h2>
         <p><%= @practices.count %> songs</p>
       </div>

       <div class="stat-card center" id="slide-3">
         <p>image</p>
         <p>Rehearsals</h2>
         <p><%= @number_of_total_sessions %></p>
       </div>

       <div class="stat-card center" id="slide-4">
         <p>image</p>
         <h2>Last Practice</h2>
         <p><%= @last_session.created_at.strftime("%A, %b %d") %></p>
            </div>

            <div class="stat-card center" id="slide-5">
        <p>image</p>
        <h2>Grade average</h2>
        <p>Average rehearsal grade: <%= @average_grade %> </p>
            </div>
     </div>


